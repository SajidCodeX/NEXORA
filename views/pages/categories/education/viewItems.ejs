<h2>Your Uploaded Files</h2>

<% if (files.length > 0) { %>
  <ul>
    <% files.forEach(file => { %>
      <li>
        <a href="<%= file.filePath %>" target="_blank">View File</a> - 
        <%= file.uploadDate.toLocaleString() %>
      </li>
    <% }) %>
  </ul>
<% } else { %>
  <p>No files uploaded yet.</p>
<% } %>
